{
  "name": "peakapp",
  "version": "1.0.0",
  "description": "Metabolomics Feature Viewer",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "nodemon --watch backend --ext py --exec \"cd backend && python3 -m uvicorn main:app --port 8000\"",
    "start-frontend": "cd frontend && npm run dev",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "keywords": [
    "metabolomics",
    "mzml",
    "viewer"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  }
}
